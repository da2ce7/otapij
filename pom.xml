    <repositories>
        <repository>
            <id>project.local</id>
            <name>project</name>
            <url>file:${project.basedir}/repo</url>
        </repository>

        <repository>
            <id>netbeans</id>
            <name>NetBeans</name>
            <url>http://bits.netbeans.org/maven2/</url>
        </repository>
        <repository>
            <id>ivy2maven2</id>
            <name>Ivy 2 Maven2</name>
            <url>http://ivy2maven2.littleware.googlecode.com/hg/maven2/</url>
        </repository>
        <repository>
            <id>jspresso</id>
            <name>Jspresso Repository</name>
            <layout>default</layout>
            <url>http://repository.jspresso.org/maven2/</url>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </repository>

    </repositories>


    <dependencies>
        <dependency>
            <groupId>chrriis</groupId>
            <artifactId>dj-nativeswing-swt</artifactId>
            <version>0.9.9.20101112</version>
