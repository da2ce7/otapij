        </dependency>
        <dependency>
            <groupId>org.netbeans.external</groupId>
            <artifactId>json-2</artifactId>
            <version>RELEASE65</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>org.jvnet</groupId>
